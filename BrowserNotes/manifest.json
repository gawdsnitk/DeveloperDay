{
  "manifest_version": 2,

  "name": "NoteThat",
  "description": "Keep your notes anytime, anywhere",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Make neat notes"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "background":{
    "scripts":["resources/js/popup.js"]
  },
  "content_security_policy":"script-src 'self'"
}